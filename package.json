{
  "name": "view-avatar",
  "version": "1.0.0",
  "description": "a vue avatar component, avatar",
  "main": "dist/index",
  "scripts": {
    "dev": "webpack-dev-server --progress --info --config webpack.dev.js",
    "build": "webpack",
    "test": "jest --watchAll --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LwcReber/view-avatar.git"
  },
  "keywords": [
    "avatar",
    "vue avatar",
    "view-avatar"
  ],
  "author": "Reber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LwcReber/view-avatar/issues"
  },
  "homepage": "https://github.com/LwcReber/view-avatar#readme",
  "dependencies": {
    "vue": "^2.5.2",
    "vue-popperjs": "^2.3.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-regenerator-runtime": "^6.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-transform-stub": "^2.0.0",
    "js-md5": "^0.7.3",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "regenerator-runtime": "^0.13.5",
    "style-loader": "^1.2.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.5.0",
    "vue-popper": "^0.3.8",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(s?css|less)$": "identity-obj-proxy"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "/vue-popperjs"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
